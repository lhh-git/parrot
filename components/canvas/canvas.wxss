/* canvas海报 */
.canvas_box {
	position: fixed;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
}
.canvas_box .mask {
	opacity:0.5;
	width: 100%;
	height: 100%;
	background:rgba(0,0,0,1);
}

.canvas_box .content {
	position: absolute;
	top: 20rpx;
	left: 50%;
	transform: translateX(-50%);
	text-align: center;
}
.canvas_box .formId {
	margin-top: 22rpx;
	line-height: 85rpx;
	width: 602rpx;
	height: 85rpx;
	font-size: 30rpx;
	letter-spacing: 4rpx;
	color: #FFFFFF;
	background: #F86442;
	border-radius: 10rpx;
}
.canvas_box .point {
	margin-top: 20rpx;
	font-size: 30rpx;
	color: #E5E5E5;
}