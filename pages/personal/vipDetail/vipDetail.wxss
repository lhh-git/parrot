page{
	background:rgba(243,243,243,1);
}

.header {
	display: flex;
	padding: 58rpx 65rpx 81rpx;
	height: 133rpx;
	background:rgba(255,255,255,1);
}
.header .photo {
	width: 133rpx;
	height: 133rpx;	
}
.header .photo image {
	width: 133rpx;
	height: 133rpx;
	border-radius: 50%;
}
.header .main {
	width: 400rpx;
}
.header .main .title {
	margin-left: 40rpx;
	margin-top: 13rpx;
	font-size: 43rpx;
	color: #4E4E4E;
}
.header .main .update_time {
	margin-left: 40rpx;
	margin-top: 18rpx;
	font-size: 23rpx;
	color: #4E4E4E;
}
.header .chat {
	flex: 1;
	margin-top: 14rpx;
}
.header .chat image {
	width: 89rpx;
	height: 81rpx;
}


.more {
	overflow: hidden;
	margin-top: 15rpx;
	height: 343rpx;
	background: #fff;
}
.more .title {
	margin-top: 38rpx;
	text-align: center;
	font-size: 30rpx;
	color: #4E4E4E;
}
.more .list {

}
.more .list .item-box {
	display: flex;
	justify-content: space-between;
	margin: 42rpx 44rpx 0 43rpx;
}
.more .list .item-box image {
	width: 318rpx;
	height: 188rpx;
	border-radius: 10rpx;
}

.order {
	display: flex;
	align-items: center;
	margin-top: 15rpx;
	width: 100%;
	height: 100rpx;
	background: #fff;
}
.order .title {
	flex: 1;
	margin-left: 57rpx;
	font-size: 26rpx;
	color: #8E8D8D;
}
.order .new {
	width: 17rpx;
	height: 17rpx;
	background:rgba(248,100,66,1);
	border-radius: 50%;
}
.order .open {
	margin: 0 64rpx 0 12rpx;
	width: 11rpx;
	height: 19rpx;
}


.privilege {
	overflow: hidden;
	margin-top: 18rpx;
	height: 228rpx;
	background: #fff;
}
.privilege .title {
	margin-top: 35rpx;
	text-align: center;
	font-size: 30rpx;
	color: #4E4E4E;
}
.privilege .list {
	display: flex;
	justify-content: space-between;
	margin: 39rpx 41rpx 0 40rpx;
}
.privilege .list .item {
	display: flex;
	overflow: hidden;
	width: 318rpx;
	height: 79rpx;
	background:#DFC06E;
	border-radius: 10rpx;
}
.privilege .list .item .left {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 94rpx;
	height: 100%;
	background: #32221A;
}
.privilege .list .item .left image {
	width: 39rpx;
	height: 39rpx;
}
.privilege .list .item .right {
	flex: 1;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 24rpx;
	color: #32221A;
}




