page {
	background: #F3F3F3;
}
.contain {
	padding-bottom: 100rpx;
}

.list {
	padding: 30rpx 68rpx 38rpx 59rpx;
	background: #fff;
}
.list:first-child {
	margin-top: 0;
}
.list .title {
	padding-bottom: 27rpx;
	font-size: 26rpx;
	color: #8E8D8D;
	border-bottom: 1rpx solid #EEEEEB;
}



.list .content {
	margin-top: 32rpx;
	padding-bottom: 34rpx;
	border-bottom: 1rpx solid #EEEEEB;
}
.list .content .item {
	display: flex;
	margin-top: 52rpx;
	height: 116rpx;
}
.list .content .item:first-child {
	margin-top: 0;
}
.list .content .item .left {
	width: 116rpx;
	height: 116rpx;
}
.list .content .item .left image {
	width: 116rpx;
	height: 116rpx;
}
.list .content .item .center {
	flex: 1;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	margin-left: 21rpx;
}
.list .content .item .center .user {
	font-size: 26rpx;
	color: #8E8D8D;
}
.list .content .item .center .count {
	display: flex;
	font-size: 22rpx;
	color: #8E8D8D;
}
.list .content .item .center .count view:nth-child(2) {
	margin-left: 8rpx;
	text-decoration: line-through;
}


.list .content .item .right {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	margin-left: 20rpx;
	text-align: right;
	font-size: 26rpx;
	color: #8E8D8D;
}
.list .content .item .right view:nth-child(2) {
	font-size: 22rpx;
}

.list .freight, .money {
	display: flex;
	justify-content: space-between;
	font-size: 26rpx;
	color: #8E8D8D;
}
.list .freight {
	margin-top: 31rpx;
}
.list .money {
	margin-top: 42rpx;
}
.list .money view:nth-child(2) {
	font-size: 22rpx;
}


.userinfo {
	margin-top: 15rpx;
	padding-bottom: 49rpx;
	background: #fff;
}
.userinfo .item {
	display: flex;
	padding-top: 49rpx;
	font-size: 26rpx;
}
.userinfo .item view:nth-child(1) {
	text-align: right;
	width: 152rpx;
	color: #8E8D8D;
}
.userinfo .item view:nth-child(2) {
	margin-left: 35rpx;
	color: #4E4E4E;
}


.logistics {
	display: flex;
	margin-top: 15rpx;
	padding-left: 54rpx;
	padding-right: 52rpx;
	height: 348rpx;
	background: #fff;
}
.logistics .status {
	margin-top: 36rpx;
}
.logistics .status .item {
	width: 40rpx;
	height: 68rpx;
	border-left: 1rpx solid#8E8D8D;
}
.logistics .status .item.active {
	border-left: 1rpx solid#4DBD44;
}
.logistics .status .round {
	margin-left: -7rpx;
	width: 15rpx;
	height: 15rpx;
	background: #8E8D8D;
	border-radius: 50%;
}
.logistics .status .round.active {
	background: #4DBD44;
}

.logistics .content {
	flex: 1;
}
.logistics .content .play {
	display: flex;
	justify-content: space-between;
	margin-top: 31rpx;
	font-size: 26rpx;
	color: #4E4E4E
}
.logistics .content .play .serve {
	display: flex;
	align-items: center;
	margin-left: 5rpx;
	font-size: 22rpx;
	color: #8E8D8D;
}
.logistics .content .play .serve image {
	width: 25rpx;
	height: 24rpx;
}
.logistics .content .time {
	margin-top: 12rpx;
	font-size: 20rpx;
	color: #8E8D8D; 
}
.logistics .content .wait {
	margin-top: 26rpx;
	font-size: 26rpx;
	color: #4E4E4E;
}
.logistics .content .send {
	margin-top: 58rpx;
	font-size: 26rpx;
	color: #4E4E4E;
}
.logistics .content .order {
	margin-top: 9rpx;
	font-size: 20rpx;
	color: #8E8D8D;
} 
.logistics .content .receive {
	margin-top: 28rpx;
	font-size: 26rpx;
	color: #4E4E4E;
}
