<view class="contain">
	<view class="header">
		<view class="photo">
			<image src="{{userInfo.avatarUrl}}"></image>
		</view>
		<view class="main">
			<view class="title overhide">{{userInfo.nickName}}</view>
			<view class="update_time">最近登陆：{{userInfo.lastTime}}</view>
		</view>
		<view class="chat">
			<image src="../../../images/my_chat.png"></image>
		</view>
	</view>

	<view class="activity">
		<navigator url="/pages/personal/listVip/listVip">
			<image src='../../../images/my_activity.png'></image>
		</navigator>
	</view>

	<view class="section">
		<navigator url="/pages/personal/income/income">
			<view>
				<image class="icon" src='../../../images/my_section1.png'></image>
				<view class="main">我的收入</view>
				<image class="open" src="../../../images/my_open.png"></image>
			</view>
		</navigator>
		<navigator url="/pages/personal/listAlbum/listAlbum">
			<view>
				<image class="icon" src='../../../images/my_section2.png'></image>
				<view class="main">我的作品</view>
				<image class="open" src="../../../images/my_open.png"></image>
			</view>
		</navigator>
		<navigator url="/pages/personal/collect/collect">
			<view>
				<image class="icon" src='../../../images/my_section3.png'></image>
				<view class="main">我的收藏</view>
				<image class="open" src="../../../images/my_open.png"></image>
			</view>
		</navigator>
		<navigator url="/pages/personal/history/history">
			<view>
				<image class="icon" src='../../../images/my_section4.png'></image>
				<view class="main">浏览记录</view>
				<image class="open" src="../../../images/my_open.png"></image>
			</view>
		</navigator>
	</view>

	<view class="section select">
		<navigator url="/pages/personal/vipDetail/vipDetail">
			<view class="news">
				<image class="icon" src='../../../images/my_section7.png'></image>
				我的畅听
			</view>
		</navigator>
		<navigator url="/pages/personal/news/news">
			<view class="news .bottom_none">
				<image class="icon" src='../../../images/my_section6.png'></image>
				我的消息
				<view class="new"></view>
			</view>
		</navigator>	
	</view>


	
	
</view>
