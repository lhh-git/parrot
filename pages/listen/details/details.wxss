.header {
	margin-top: 25rpx;
	overflow: hidden;
	position: relative;
	box-shadow: 0rpx 12rpx 5rpx #e9e9e9;
}
.header>.photo {
	position: absolute;
	left: 50%;
	top: 0;
	transform: translateX(-50%);

}
.header>.photo image {
	width: 687rpx;
	height: 310rpx;
}
.header .play {
	margin-top: 360rpx;
	height: 82rpx;
}
.header .play image:nth-child(1){
	margin-left: 204rpx;
	width: 30rpx;
	height: 29rpx;
}
.header .play image:nth-child(2){
	margin-left: 100rpx;
	width: 82rpx;
	height: 82rpx;
}
.header .play image:nth-child(3){
	margin-left: 99rpx;
	width: 30rpx;
	height: 29rpx;
}
.header .play image:nth-child(4){
	margin-left: 92rpx;
	width: 39rpx;
	height: 36rpx;
}
.header .progress {
	margin: 51rpx 51rpx 0 44rpx;
}
.header .all_show {
	position: relative;
	height: 2rpx;
	background: #989898;
}
.header .all_show .show {
	position: absolute;
	height: 100%;
	background: #FF5B36;
}
.header .all_show .show .icon {
	position: absolute;
	right: 0;
	top: -5rpx;
	width: 12rpx;
	height: 12rpx;
	border-radius: 12rpx;
	background: #FF5B36;
}
.header .time {
	display: flex;
	justify-content: space-between;
	margin-top: 19rpx;
	font-size: 25rpx;
	color: #838383;
}
.header .subscribe {
	position: relative;
	display: flex;
	margin: 40rpx 52rpx 0 44rpx;
	padding-bottom: 30rpx;
}
.header .subscribe>.photo {
	width: 60rpx;
	height: 60rpx;
}
.header .subscribe>.photo>image {
	width: 60rpx;
	height: 60rpx;
}
.header .subscribe>.main {
	padding-left: 13rpx;
}
.header .subscribe>.main>view:nth-child(1) {
	font-size: 25rpx;
	color: #4E4E4E;
}
.header .subscribe>.main>view:nth-child(2) {
	margin-top: 15rpx;
	font-size: 20rpx;
	color: #8E8D8D;
}
.header .subscribe>.button {
	position: absolute;
	right: 0;
	width: 196rpx;
	height: 63rpx;
}
.header .subscribe>.button>image {
	width: 196rpx;
	height: 63rpx;
}

.header .subscribe>.button .form_id {
	line-height: 63rpx;
	width: 196rpx;
	height: 63rpx;
	font-size: 33rpx;
	color: #FFFFFF;
	background: #D2D2D2;
	border-radius: 63rpx;
}




.section {
    padding-bottom: 100rpx;
}
.section .synopsis {
	margin: 38rpx 59rpx 0 56rpx;
}
.section .synopsis .title {
	font-size: 30rpx;
	font-weight: bold;
	color: #4E4E4E;
}
.section .synopsis .content {
	margin-top: 4rpx;
	overflow: hidden;
	line-height: 34rpx;
	height: 102rpx;
	font-size: 25rpx;
	letter-spacing: 4rpx;
	color: #8E8D8D;	
}
.section .synopsis .active {
	margin-top: 4rpx;
	line-height: 34rpx;
	font-size: 25rpx;
	letter-spacing: 4rpx;
	color: #8E8D8D;
}
.section .synopsis .show {
	display: flex;
	align-items: center;
	padding: 10rpx 0;
	height: 23rpx;
	font-size: 23rpx;
	font-weight: bold;
	color: #8E8D8D;
}
.section .synopsis .show image {
	margin-left: 7rpx;
	width: 21rpx;
	height: 14rpx;
}



/* 专辑 */
.complete {
	margin: 46rpx 53rpx 0 55rpx;
}
.complete .tool {
	display: flex;
	justify-content: space-between;
	align-items: center;
	height: 63rpx;
}
.complete .tool .tool_info {
	display: flex;
	font-size: 30rpx;
	font-weight: bold;
	color: #4E4E4E;
}
.complete .tool .tool_info .status {
	text-align: center;
	line-height: 31rpx;
	width: 31rpx;
	height: 31rpx;
	font-size: 22rpx;
	color: white;
	background: #f86442;
	border-radius: 6rpx;
}
.complete .tool .button .form_id{
	line-height: 63rpx;
	width: 196rpx;
	height: 63rpx;
	font-size: 33rpx;
	color: #FFFFFF;
	background: #D2D2D2;
	border-radius: 63rpx;
}

.complete .list {
	overflow: hidden;
	overflow-x: auto;
	display: flex;
	margin-top: 15rpx; 
}
.complete .list .item {
	margin-left: 21rpx;
	width: 142rpx;
    text-align:center;
}
.complete .list .item:first-child {
	margin-left: 0;
}
.complete .list .item .photo {
	position: relative;
	width: 142rpx;
	height: 142rpx;
}
.complete .list .item .photo image {
	width: 142rpx;
	height: 142rpx;
	border-radius: 15rpx;
}
.complete .list .item .photo .photo_sel {
	position: absolute;
	left: 0;
	top: 0;
}
.complete .list .item .item_info {
	display: flex;
	justify-content: space-between;
	margin-left: 11rpx;
	margin-top: 10rpx;
}
.complete .list .item .item_info .sort {
	font-size: 25rpx;
	color: #4E4E4E;
}
.complete .list .item .item_info .sort.active{
	color: #F86442;
}
.complete .list .item .item_info .time {
	margin-top: 6rpx;
	font-size: 23rpx;
	color: #C9C9C9;
}





/* 相关推荐 */
.section .recommend {
	margin-top: 58rpx;
}
.section .recommend .title {
	margin-left: 57rpx;
	font-size: 30rpx;
	color: #4E4E4E;
}
.section .recommend .list {
	display: flex;
	flex-wrap: wrap;
	margin: 35rpx 86rpx 0 85rpx;
	padding-bottom: 100rpx;
}
.section .recommend .list .item {
	width: 142rpx;
	margin-bottom: 22rpx;
    position: relative;
}
.section .recommend .list .item:nth-child(3n-1) {
	margin: 0 77rpx 0 76rpx;
}
.section .recommend .list .item image {
	width: 142rpx;
	height: 142rpx;
	border-radius: 15rpx;
}
.section .recommend .list .item .words {
	margin: 0 10rpx;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	line-height: 35rpx;
	
	font-size: 25rpx;
	color: #4E4E4E;

}
.photo_sel {
	position: absolute;
	left: 0;
	top: 0;
}

/* 收藏 */
.collect {
    width: 100%;
    display: flex;
    /* justify-content: space-between; */
    font-size: 30rpx;
    margin-top:20rpx;
    position: relative;
    margin-bottom:20rpx;
}
.collect image{
    width: 30rpx;
    height: 30rpx;
    display: block;
}
.collect_item{
    display: flex;
    margin-left:44rpx; 
}
.collect_item:last-child{
  position: absolute;
  right:44rpx;
}
.right{
    padding-right: 20rpx;
}
